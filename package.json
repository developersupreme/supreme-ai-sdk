{
  "name": "@supreme-ai/credit-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Supreme AI Credit System - Supports both embedded (iframe) and standalone modes",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "credit-system",
    "jwt",
    "iframe",
    "postMessage",
    "api-sdk",
    "supreme-ai",
    "typescript",
    "react"
  ],
  "author": "Supreme AI",
  "license": "MIT",
  "homepage": "https://github.com/developersupreme/supreme-credit-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developersupreme/supreme-credit-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/developersupreme/supreme-credit-sdk/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
